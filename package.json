    "react-dom": "18.1.0",
    "react-icons": "^4.4.0",
    "react-infinite-scroll-component": "^6.1.0",
    "react-spinners": "^0.12.0",
    "type-graphql": "^1.1.1",
    "wagmi": "^0.4.12"
  },
  "devDependencies": {
    "@types/micro-cors": "^0.1.2",
    "@types/node": "17.0.41",
    "@types/react": "18.0.12",
    "@types/react-dom": "18.0.5",
    "@types/react-infinite-scroll-component": "^5.0.0",
    "autoprefixer": "^10.4.7",
