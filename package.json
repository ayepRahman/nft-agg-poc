{
  "name": "nft-agg-poc",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "gen-gql": "npm run gen-gql:client && npm run gen-gql:server",
    "gen-gql:client": "npm run gen-gql:client-queries",
    "gen-gql:client-queries": "graphql-codegen --config=codegen.client.queries.yml --require dotenv/config",
    "gen-gql:client-mutation": "graphql-codegen --config=codegen.client.mutation.yml --require dotenv/config",
    "gen-gql:server": "graphql-codegen --config=codegen.server.yml --require dotenv/config"
  },
  "dependencies": {
    "@apollo/client": "^3.6.6",
    "@chakra-ui/react": "^2.2.1",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/introspection": "^2.1.1",
    "@graphql-codegen/typescript": "^2.5.0",
    "@graphql-codegen/typescript-operations": "^2.4.1",
    "@graphql-codegen/typescript-react-apollo": "^3.2.15",
    "@graphql-codegen/typescript-resolvers": "^2.6.5",
    "@graphql-tools/graphql-file-loader": "^7.3.15",
    "@graphql-tools/load": "^7.5.14",
    "@svgr/webpack": "^6.2.1",
